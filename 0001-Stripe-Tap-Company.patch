+
+interface LanguageLayoutProps {
+  children: React.ReactNode;
+  params: Promise<{ lang: Language }>;
+}
+
+export async function generateStaticParams() {
+  return i18nConfig.supportedLanguages.map((lang) => ({ lang }));
+}
+
+export async function generateMetadata({
+  params,
+}: {
+  params: Promise<{ lang: Language }>;
+}): Promise<Metadata> {
+  const { lang } = await params;
+  const isArabic = lang === 'ar';
+
+  return {
+    title: isArabic
+      ? 'ุงุฎุชุจุงุฑุงุช ุงูุฃููุงู ูููุดู ุนู ุงููุฎุฏุฑุงุช ูุงููุคุซุฑุงุช ุงูุนูููุฉ'
+      : 'Color Testing for Drug and Psychoactive Substance Detection',
+    description: isArabic
+      ? 'ูุธุงู ูุชูุฏู ูุงุฎุชุจุงุฑ ุงูููุงุฏ ูุงููุคุซุฑุงุช ุงูุนูููุฉ ุจุงุณุชุฎุฏุงู ุงูููุงุดู ุงูููููุงุฆูุฉ'
+      : 'Advanced system for drug and psychoactive substance testing using chemical reagents',
+    alternates: {
+      canonical: `/${lang}`,
+      languages: {
+        'ar': '/ar',
+        'en': '/en',
+      },
+    },
+    openGraph: {
+      locale: isArabic ? 'ar_SA' : 'en_US',
+      alternateLocale: isArabic ? 'en_US' : 'ar_SA',
+    },
+  };
+}
+
+export default async function LanguageLayout({
+  children,
+  params,
+}: LanguageLayoutProps) {
+  const { lang } = await params;
+  // Validate language parameter
+  if (!i18nConfig.supportedLanguages.includes(lang)) {
+    notFound();
+  }
+
+  const direction = getTextDirection(lang);
