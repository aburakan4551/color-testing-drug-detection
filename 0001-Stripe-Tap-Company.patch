+    status: string;
+    url: string;
+  };
+}
+
+interface TapSubscriptionRequest {
+  term: {
+    interval: string;
+    period: number;
+    from: string;
+    due: string;
+    auto_renew: boolean;
+  };
+  trial?: {
+    period: number;
