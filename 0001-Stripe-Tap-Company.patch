+          data.dailyUsage.reduce((sum, day) => sum + day.count, 0) / 
+          data.dailyUsage.length
+        ).toString()
+      ]
+    ];
+
+    doc.autoTable({
+      startY: yPosition,
+      head: [[
+        isRTL ? 'ุงููุคุดุฑ' : 'Metric',
+        isRTL ? 'ุงููููุฉ' : 'Value'
+      ]],
+      body: statsData,
+      theme: 'grid',
+      styles: {
+        fontSize: 10,
+        cellPadding: 5,
+        textColor: [40, 40, 40]
+      },
+      headStyles: {
+        fillColor: [66, 139, 202],
+        textColor: [255, 255, 255],
+        fontStyle: 'bold'
+      },
+      margin: { left: margin, right: margin },
+      tableWidth: 'auto'
+    });
+
+    yPosition = (doc as any).lastAutoTable.finalY + 20;
+
+    // Popular Tests Section
+    if (data.popularTests.length > 0) {
+      doc.setFontSize(14);
+      doc.setTextColor(40, 40, 40);
+      
+      const popularTitle = isRTL 
+        ? 'ุงูุงุฎุชุจุงุฑุงุช ุงูุฃูุซุฑ ุงุณุชุฎุฏุงูุงู' 
