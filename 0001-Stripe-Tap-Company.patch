+    price: 29,
+    currency: 'SAR',
+    interval: 'month',
+    features: [
+      'ูุตูู ุบูุฑ ูุญุฏูุฏ ูุฌููุน ุงูุงุฎุชุจุงุฑุงุช',
+      'ุชุญุฏูุซุงุช ุฏูุฑูุฉ ููุงุฎุชุจุงุฑุงุช',
+      'ุฏุนู ููู ูุชูุฏู',
+      'ุชูุงุฑูุฑ ููุตูุฉ',
+      'ุญูุธ ุชุงุฑูุฎ ุงูุงุฎุชุจุงุฑุงุช'
+    ]
+  }
+];
+
+interface TapChargeRequest {
+  amount: number;
+  currency: string;
+  customer?: {
+    first_name: string;
+    last_name: string;
+    email: string;
+    phone?: {
+      country_code: string;
