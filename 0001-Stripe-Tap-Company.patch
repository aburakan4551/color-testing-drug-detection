+    yPosition += 20;
+
+    // Summary Statistics
+    doc.setFontSize(14);
+    doc.setTextColor(40, 40, 40);
+    
+    const summaryTitle = isRTL ? 'ููุฎุต ุงูุฅุญุตุงุฆูุงุช' : 'Summary Statistics';
+    
+    if (isRTL) {
+      doc.text(summaryTitle, pageWidth - margin, yPosition, { align: 'right' });
+    } else {
+      doc.text(summaryTitle, margin, yPosition);
+    }
+    
+    yPosition += 15;
+
+    // Statistics table
+    const statsData = [
+      [
+        isRTL ? 'ุฅุฌูุงูู ุงูุงุฎุชุจุงุฑุงุช' : 'Total Tests',
+        data.totalTests.toString()
+      ],
+      [
+        isRTL ? 'ุฃููุงุน ุงูุงุฎุชุจุงุฑุงุช' : 'Test Types',
+        Object.keys(data.testsByType).length.toString()
