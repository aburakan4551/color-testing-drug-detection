+        head: [[
+          isRTL ? 'ุงุณู ุงูุงุฎุชุจุงุฑ' : 'Test Name',
+          isRTL ? 'ุนุฏุฏ ุงูุงุณุชุฎุฏุงูุงุช' : 'Usage Count',
+          isRTL ? 'ุงููุณุจุฉ ุงููุฆููุฉ' : 'Percentage'
+        ]],
+        body: popularTestsData,
+        theme: 'striped',
+        styles: {
+          fontSize: 10,
+          cellPadding: 5,
+          textColor: [40, 40, 40]
+        },
+        headStyles: {
+          fillColor: [92, 184, 92],
+          textColor: [255, 255, 255],
+          fontStyle: 'bold'
+        },
+        margin: { left: margin, right: margin }
+      });
+
+      yPosition = (doc as any).lastAutoTable.finalY + 20;
+    }
+
+    // Tests by Type Section
+    if (Object.keys(data.testsByType).length > 0) {
+      // Check if we need a new page
+      if (yPosition > pageHeight - 80) {
+        doc.addPage();
+        yPosition = margin;
+      }
+
+      doc.setFontSize(14);
+      doc.setTextColor(40, 40, 40);
+      
+      const testsByTypeTitle = isRTL 
+        ? 'ุงูุงุฎุชุจุงุฑุงุช ุญุณุจ ุงูููุน' 
