+    url: string;
+  };
+  description: string;
+  metadata?: Record<string, any>;
+  receipt?: {
+    email: boolean;
+    sms: boolean;
+  };
+}
+
+interface TapChargeResponse {
+  id: string;
+  object: string;
+  live_mode: boolean;
+  amount: number;
+  currency: string;
+  description: string;
+  status: string;
+  response: {
+    code: string;
+    message: string;
+  };
+  reference: {
+    transaction: string;
+    order: string;
+  };
+  transaction: {
+    timezone: string;
+    created: string;
+    url: string;
+    expiry: {
+      period: number;
+      type: string;
+    };
+    asynchronous: boolean;
+    amount: number;
+    currency: string;
+  };
+  receipt: {
+    email: boolean;
+    sms: boolean;
+  };
+  customer: {
+    first_name: string;
+    last_name: string;
+    email: string;
+    phone: {
+      country_code: string;
+      number: string;
