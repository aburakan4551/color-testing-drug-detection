+      
+      if (isRTL) {
+        doc.text(popularTitle, pageWidth - margin, yPosition, { align: 'right' });
+      } else {
+        doc.text(popularTitle, margin, yPosition);
+      }
+      
+      yPosition += 15;
+
+      const popularTestsData = data.popularTests.map(test => [
